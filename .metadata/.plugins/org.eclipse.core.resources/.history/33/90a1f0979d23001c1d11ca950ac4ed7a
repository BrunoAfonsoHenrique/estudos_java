package desafio;

public class contaTeste {

	public static void main(String[] args) {
		
		ContaBanco joao = new ContaBanco(false, 0);
		joao.setDono("João");
		joao.setTipoConta("CC");
		joao.abrirConta();
		joao.depositar(300);
		joao.pagarMensal();
		joao.sacar();
		joao.sacar();
		joao.sacar();
		joao.sacar();
		joao.sacar();
		joao.sacar();
		joao.sacar();
		joao.status();
	}

}
