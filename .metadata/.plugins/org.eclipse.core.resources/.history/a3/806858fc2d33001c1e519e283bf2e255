package projetofinalpoo;

public class Gafanhoto extends Pessoa{
	
	private String login;
	private int totalAssistindo;
	
	public Gafanhoto(String nome, int idade, String sexo, String login) {
		super(nome, idade, sexo);
		this.setLogin(login);
		this.setTotalAssistindo(0);
	}

	public String getLogin() {
		return login;
	}

	public void setLogin(String login) {
		this.login = login;
	}

	public int getTotalAssistindo() {
		return totalAssistindo;
	}

	public void setTotalAssistindo(int totalAssistindo) {
		this.totalAssistindo = totalAssistindo;
	}
	
	@Override
	public String toString() {
		return "\n ----------------------------\n"
				+ " Gafanhoto" + super.toString() + "[ nome = " + nome + 
				"\n idade = " + idade + 
				"\n sexo = " + sexo + 
				"\n experiencia = " + experiencia + " ]";
	}
}
